{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RoomStateResponse",
  "type": "object",
  "required": [
    "bet_amount",
    "entropy",
    "lucky_number",
    "position",
    "prediction_number",
    "results",
    "start_time"
  ],
  "properties": {
    "bet_amount": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "entropy": {
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    },
    "lucky_number": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "position": {
      "type": "string"
    },
    "prediction_number": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "results": {
      "type": "boolean"
    },
    "start_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
